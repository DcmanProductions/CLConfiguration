<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>ChaseLabs.CLConfiguration</RootNamespace>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.0.1</Version>
    <Authors>Drew Chase</Authors>
    <Company>Chase Labs</Company>
    <Description>A Easy to Use Config Manager</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/DcmanProductions/CLConfiguration.git</RepositoryUrl>
    <RepositoryType>Git Hub</RepositoryType>
    <PackageProjectUrl>https://github.com/DcmanProductions/CLConfiguration</PackageProjectUrl>
    <PackageReleaseNotes>Initial Release</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Remove=".gitignore" />
    <None Remove="LICENSE" />
  </ItemGroup>

</Project>
